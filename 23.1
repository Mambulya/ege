def F(n, t):
    operations = [0]*n + [1] + [0]*(t-n)

    for i in range(n + 1, t+1):
        operations[i] += operations[i-1]

        if not(i % 2) and (i <= 6 or i == 14 or i >= 28):
            operations[i] += operations[i // 2]
        if not(i % 3) and i <= 6:
            operations[i] += operations[i // 3]
        print(i, operations[i])
    print(operations)
    return operations[-1]

print(F(1, 30))
