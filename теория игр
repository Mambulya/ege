for s in range(1, 84):
    p = s
    v1 = p + 1
    v2 = p + 2
    v3 = 4*p - 1
    if any((v1 + 1 >= 85, v1 + 2 >= 85, v1*4 - 1 >= 85)) and any((v2 + 1 >= 85, v2 + 2 >= 85, v2*4 - 1 >= 85)) and any((v3 + 1 >= 85, v3 + 2 >= 85, v3*4 - 1 >= 85)) and all((v1 < 85, v2 < 85, v3 < 85)):
        print(s)
